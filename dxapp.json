{
  "name": "multiqc",
  "title": "eggd_multiqc",
  "summary": "multiqc",
  "dxapi": "1.0.0",
  "version": "1.0.0",
  "inputSpec": [
    {
      "name": "eggd_multiqc_config_file",
      "class": "file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "html_report",
      "class": "file",
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "multiqc_data_files",
      "class": "array:file",
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 24
      }
    },
    "interpreter": "bash",
    "release": "16.04",
    "distribution": "Ubuntu",
    "file": "src/multiqc.sh"
  },
  "access": {
    "network": [
      "*"
    ]
  },
  "regionalOptions": {
    "aws:us-east-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_v2_x4"
        }
      }
    }
  }
}
